@model List<MovieList>
@{
    ViewData["Title"] = "List";
}
<h2>MovieList</h2>
<table>
    <tr>
        <th>Id</th>
        <th>Title</th>
        <th>BoxOffice</th>
        <th>ActiveStatus</th>
        <th>DateOfLaunch</th>
        <th>Genere</th>
        <th>Teaser</th>
    </tr>
    @foreach (var movie in Model)
            {
                <tr>
                 <td>@movie.Id</td>
                <td>@movie.Title</td>
                <td>@movie.BoxOffice</td>
                <td>@movie.ActiveStatus</td>
                <td>@movie.DateOfLaunch</td>
                <td>@movie.SelectGenere</td>
                <td>@movie.MovieGenere</td>
                <td>@movie.hasTeaser</td>
                </tr>
            }
</table>
