@model List<MovieList>
@{
    ViewData["Title"] = "AdminPage";
}
<h2>MovieList</h2>
@Html.ActionLink("Create", "Create", "Movie");
<table>
    <tr>
        <th>Id</th>
        <th>Title</th>
        <th>BoxOffice</th>
        <th>ActiveStatus</th>
        <th>DateOfLaunch</th>
        <th>Genere</th>
        <th>Teaser</th>
    </tr>
    @foreach (var movie in Model)
    {
        <tr>
            <td>@movie.Id</td>
            <td>@movie.Title</td>
            <td>@movie.BoxOffice</td>
            <td>@movie.ActiveStatus</td>
            <td>@movie.DateOfLaunch</td>
            <td>@movie.SelectGenere</td>
            <td>@movie.MovieGenere</td>
            <td>@movie.hasTeaser</td>
            <td>

                @Html.ActionLink("Details", "Details", "Movie", new { id = movie.Id })
                @Html.ActionLink("Edit Product", "Edit", "Movie", new { id = movie.Id })
                @Html.ActionLink("Remove", "Delete", "Movie", new { id = movie.Id })
            </td>
        </tr>
    }
</table>
