@model MovieApp_UsingMvc.Models.Registration
@{
    ViewData["Title"] = "Index";
}
<!DOCTYPE html>
<html>
<head>
    <style>
        ul {
            list-style-type: none;
            margin: 0;
            padding: 0;
            overflow: hidden;
            background-color: #333;
        }

        li {
            float: left;
        }

            li a {
                display: block;
                color: white;
                text-align: center;
                padding: 14px 16px;
                text-decoration: none;
            }

                /* Change the link color to #111 (black) on hover */
                li a:hover {
                    background-color: #111;
                }
    </style>
</head>
<body>

    <ul>
        <li><a asp-area="" asp-controller="Movie" asp-action="List">ListOfMovies</a></li>
        <li><a asp-area="" asp-controller="Movie" asp-action="AdminPage">AdminPage</a></li>
        

    </ul>
    <h2>Welcome To the BoxOffice!!!!</h2>

</body>
</html>
